{
  "name": "juliangut/sessionware",
  "description": "PSR7 session creation management middleware",
  "keywords": [
    "psr7",
    "session",
    "middleware"
  ],
  "homepage": "https://github.com/juliangut/sessionware",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Julián Gutiérrez",
      "email": "juliangut@gmail.com",
      "homepage": "http://www.juliangut.com",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/juliangut/sessionware",
    "issues": "https://github.com/juliangut/sessionware/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5",
    "symfony/polyfill-php56": "^1.0",
    "symfony/polyfill-php70": "^1.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpmd/phpmd": "^2.2",
    "phpunit/phpunit": "^4.5|^5.0",
    "sebastian/phpcpd": "^2.0",
    "sensiolabs/security-checker": "^3.0",
    "squizlabs/php_codesniffer": "^2.0",
    "zendframework/zend-diactoros": "^1.3"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "Jgut\\Middleware\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jgut\\Middleware\\Sessionware\\Tests\\": "tests/Middleware/"
    }
  },
  "bin": [
  ],
  "config": {
    "preferred-install": "dist"
  }
}
